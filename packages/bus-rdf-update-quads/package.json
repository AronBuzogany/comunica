{
  "name": "@comunica/bus-rdf-update-quads",
  "version": "2.0.0",
  "description": "A comunica bus for rdf-update-quads events.",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/bus-rdf-update-quads"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "bus",
    "rdf-update-quads"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "dependencies": {
    "@comunica/bus-http": "^2.0.0",
    "@comunica/context-entries": "^2.0.0",
    "@comunica/core": "^2.0.0",
    "@rdfjs/types": "*",
    "asynciterator": "^3.2.1",
    "stream-to-string": "^1.2.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
