  "@id": "npmd:@comunica/core",
  "components": [
    {
      "@id": "cc:lib/Actor#Actor",
      "@type": "AbstractClass",
      "requireElement": "Actor",
      "extends": [
        "cc:lib/Actor#IActorArgs"
      ],
      "comment": "An actor can act on messages of certain types and provide output of a certain type.  The flow of an actor is as follows: 1. Send a message to {@link Actor#test} to test if an actor can run that action. 2. If the actor can reply to the message, let the actor run the action using {@link Actor#run}.  An actor is typically subscribed to a bus, using which the applicability to an action can be tested.",
      "parameters": [
        {
          "@id": "cc:lib/Actor#Actor_args_name",
          "range": "xsd:string",
          "default": {
            "@id": "rdf:subject"
          },
          "comment": "The name for this actor.",
          "unique": true,
          "required": true
        },
        {
          "@id": "cc:lib/Actor#Actor_args_bus",
          "range": "cc:lib/Bus#Bus",
          "comment": "The bus this actor subscribes to.",
          "unique": true,
          "required": true
        },
        {
          "@id": "cc:lib/Actor#Actor_args_beforeActors",
          "range": "cc:lib/Actor#Actor",
          "comment": "Actor that must be registered in the bus before this actor."
        }
      ],
      "constructorArguments": [
        {
          "@id": "cc:lib/Actor#Actor_args__constructorArgument",
          "fields": [
            {
              "keyRaw": "name",
              "value": {
                "@id": "cc:lib/Actor#Actor_args_name"
              }
            },
            {
              "keyRaw": "bus",
              "value": {
                "@id": "cc:lib/Actor#Actor_args_bus"
              }
            },
            {
              "keyRaw": "beforeActors",
              "value": {
                "@id": "cc:lib/Actor#Actor_args_beforeActors"
              }
            }
          ]
        }
      ]
    },
