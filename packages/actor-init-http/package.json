{
  "name": "@comunica/actor-init-http",
  "version": "2.0.0",
  "description": "A Comunica engine for triggering HTTP requests",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/actor-init-http"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "runner"
  ],
  "license": "MIT",
  "bin": {
    "comunica-http": "./bin/run.js"
  },
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "config",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "bin/**/*.d.ts",
    "bin/**/*.js"
  ],
  "dependencies": {
    "@comunica/actor-http-node-fetch": "^2.0.0",
    "@comunica/bus-http": "^2.0.0",
    "@comunica/bus-init": "^2.0.0",
    "@comunica/core": "^2.0.0",
    "@comunica/mediator-number": "^2.0.0",
    "@comunica/runner": "^2.0.0",
    "@comunica/runner-cli": "^2.0.0"
  },
  "devDependencies": {
    "@comunica/bus-http": "^1.2.0",
    "@comunica/bus-init": "^1.2.0",
    "@comunica/core": "^1.2.0",
    "@comunica/mediatortype-time": "^2.0.0",
    "cross-fetch": "^3.0.5"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "npm run build:ts && npm run build:components",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "build:components": "componentsjs-generator",
    "validate": "npm ls"
  }
}
