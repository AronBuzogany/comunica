          "required": true
        },
        {
          "@id": "cmn:lib/MediatorNumber#MediatorNumber_args_ignoreErrors",
          "range": "xsd:boolean",
          "comment": "If actors that throw test errors should be ignored",
          "unique": true
        },
        {
