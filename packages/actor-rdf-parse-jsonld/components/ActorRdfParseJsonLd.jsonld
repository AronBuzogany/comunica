      "@type": "Class",
      "requireElement": "ActorRdfParseJsonLd",
      "extends": [
        "cbrp:components/ActorRdfParseFixedMediaTypes.jsonld#ActorRdfParseFixedMediaTypes"
      ],
      "comment": "A JSON-LD RDF Parse actor that listens on the 'rdf-parse' bus.  It is able to parse JSON-LD-based RDF serializations and announce the presence of them by media type.",
      "parameters": [
        {
          "@id": "carpj:components/ActorRdfParseJsonLd.jsonld#ActorRdfParseJsonLd_args_mediatorHttp",
          "range": "cc:components/Mediator.jsonld#Mediator",
          "comment": "The HTTP mediator"
        },
        {
          "@id": "carpj:components/ActorRdfParseJsonLd.jsonld#ActorRdfParseJsonLd_args_mediaTypePriorities",
          "range": "rdf:JSON",
          "default": {
            "@type": "@json",
            "@value": {
              "application/ld+json": 1,
              "application/json": 0.5
            }
          },
          "comment": "A record of media types, with media type name as key, and its priority as value. Priorities are numbers between [0, 1]."
        },
        {
          "@id": "carpj:components/ActorRdfParseJsonLd.jsonld#ActorRdfParseJsonLd_args_mediaTypeFormats",
          "range": "rdf:JSON",
          "default": {
            "@type": "@json",
            "@value": {
              "application/ld+json": "http://www.w3.org/ns/formats/JSON-LD",
              "application/json": "http://www.w3.org/ns/formats/JSON-LD"
            }
          },
          "comment": "A record of media types, with media type name as key, and its format IRI as value."
        },
        {
          "@id": "carpj:components/ActorRdfParseJsonLd.jsonld#ActorRdfParseJsonLd_args_priorityScale",
          "range": {
            "@type": "ParameterRangeUnion",
            "parameterRangeElements": [
              "xsd:double",
              {
                "@type": "ParameterRangeUndefined"
              }
            ]
          },
          "comment": "A multiplier for media type priorities. This can be used for keeping the original media types in place, but scaling all of their scores with a certain value."
        },
        {
          "@id": "carpj:components/ActorRdfParseJsonLd.jsonld#ActorRdfParseJsonLd_args_name",
          "range": "xsd:string",
          "default": {
            "@id": "rdf:subject"
          },
          "comment": "The name for this actor."
        },
        {
          "@id": "carpj:components/ActorRdfParseJsonLd.jsonld#ActorRdfParseJsonLd_args_bus",
          "range": "cc:components/Bus.jsonld#Bus",
          "default": {
            "@id": "cbrp:components/ActorRdfParse.jsonld#ActorRdfParse_default_bus",
            "@type": "cc:components/Bus.jsonld#Bus"
          },
          "comment": "The bus this actor subscribes to."
        },
        {
          "@id": "carpj:components/ActorRdfParseJsonLd.jsonld#ActorRdfParseJsonLd_args_beforeActors",
          "range": {
            "@type": "ParameterRangeUnion",
            "parameterRangeElements": [
              {
                "@type": "ParameterRangeArray",
                "parameterRangeValue": "cc:components/Actor.jsonld#Actor"
              },
              {
                "@type": "ParameterRangeUndefined"
              }
            ]
          },
          "comment": "Actor that must be registered in the bus before this actor."
        }
      ],
