{
  "name": "@comunica/actor-query-operation-update-drop",
  "version": "1.22.0",
  "description": "A drop query-operation actor",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/actor-query-operation-update-drop"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "actor",
    "query-operation",
    "drop"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "dependencies": {
    "rdf-data-factory": "^1.0.4"
  },
  "peerDependencies": {
    "@comunica/bus-query-operation": "^1.19.2",
    "@comunica/core": "^1.19.2"
  },
  "devDependencies": {
    "@comunica/bus-query-operation": "^1.22.0",
    "@comunica/core": "^1.22.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "npm run build:ts && npm run build:components-actor",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "build:components-actor": "componentsjs-generator -s lib --typeScopedContexts -i ../../.componentsignore",
    "validate": "npm ls"
  }
}
