  "components": [
    {
      "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd",
      "@type": "Class",
      "requireElement": "ActorRdfParseJsonLd",
      "extends": [
        "cbrp:lib/ActorRdfParseFixedMediaTypes#ActorRdfParseFixedMediaTypes"
      ],
      "comment": "A JSON-LD RDF Parse actor that listens on the 'rdf-parse' bus.  It is able to parse JSON-LD-based RDF serializations and announce the presence of them by media type.",
      "parameters": [
        {
          "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd_args_mediatorHttp",
          "range": "cc:lib/Mediator#Mediator",
          "comment": "The HTTP mediator",
          "unique": true,
          "required": true
        },
        {
          "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd_args_mediaTypePriorities",
          "range": "rdf:JSON",
          "default": [
            {
              "@type": "@json",
              "@value": {
                "application/ld+json": 1,
                "application/json": 0.5
              }
            }
          ],
          "comment": "A record of media types, with media type name as key, and its priority as value. Priorities are numbers between [0, 1].",
          "unique": true,
          "required": true
        },
        {
          "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd_args_mediaTypeFormats",
          "range": "rdf:JSON",
          "default": [
            {
              "@type": "@json",
              "@value": {
                "application/ld+json": "http://www.w3.org/ns/formats/JSON-LD",
                "application/json": "http://www.w3.org/ns/formats/JSON-LD"
              }
            }
          ],
          "comment": "A record of media types, with media type name as key, and its format IRI as value.",
          "unique": true,
          "required": true
        },
        {
          "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd_args_priorityScale",
          "range": "xsd:double",
          "comment": "A multiplier for media type priorities. This can be used for keeping the original media types in place, but scaling all of their scores with a certain value.",
          "unique": true
        },
        {
          "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd_args_name",
          "range": "xsd:string",
          "default": [
            {
              "@id": "rdf:subject"
            }
          ],
          "comment": "The name for this actor.",
          "unique": true,
          "required": true
        },
        {
          "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd_args_bus",
          "range": "cc:lib/Bus#Bus",
          "default": [
            {
              "@id": "cbrp:lib/ActorRdfParse#ActorRdfParse_default_bus",
              "@type": "cc:lib/Bus#Bus"
            }
          ],
          "comment": "The bus this actor subscribes to.",
          "unique": true,
          "required": true
        },
        {
          "@id": "carpj:lib/ActorRdfParseJsonLd#ActorRdfParseJsonLd_args_beforeActors",
          "range": "cc:lib/Actor#Actor",
          "comment": "Actor that must be registered in the bus before this actor."
        }
      ],
      "constructorArguments": [
        {
