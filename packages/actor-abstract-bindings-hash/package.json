{
  "name": "@comunica/actor-abstract-bindings-hash",
  "version": "1.22.0",
  "description": "An abstract actor for handling binding query operators",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/actor-abstract-bindings-hash"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "actor",
    "abstract",
    "binding",
    "hash"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "peerDependencies": {
    "@comunica/bus-query-operation": "^1.0.0",
    "@comunica/core": "^1.0.0"
  },
  "devDependencies": {
    "@comunica/bus-query-operation": "^1.22.0",
    "@comunica/core": "^1.22.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "npm run build:ts && npm run build:components-aux",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "build:components-aux": "componentsjs-generator",
    "validate": "npm ls"
  },
  "dependencies": {
    "@comunica/types": "^1.22.0",
    "canonicalize": "^1.0.1",
    "hash.js": "^1.1.7",
    "rdf-string": "^1.5.0",
    "sparqlalgebrajs": "^4.0.0"
  }
}
