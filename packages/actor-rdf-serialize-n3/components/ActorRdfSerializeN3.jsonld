  "components": [
    {
      "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3",
      "@type": "Class",
      "requireElement": "ActorRdfSerializeN3",
      "extends": [
        "cbrs:lib/ActorRdfSerializeFixedMediaTypes#ActorRdfSerializeFixedMediaTypes"
      ],
      "comment": "A comunica N3 RDF Serialize Actor.",
      "parameters": [
        {
          "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3_args_mediaTypePriorities",
          "range": "rdf:JSON",
          "default": [
            {
              "@type": "@json",
              "@value": {
                "application/n-quads": 1,
                "application/trig": 0.95,
                "application/n-triples": 0.8,
                "text/turtle": 0.6,
                "text/n3": 0.35
              }
            }
          ],
          "comment": "A record of media types, with media type name as key, and its priority as value. Priorities are numbers between [0, 1].",
          "unique": true,
          "required": true
        },
        {
          "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3_args_mediaTypeFormats",
          "range": "rdf:JSON",
          "default": [
            {
              "@type": "@json",
              "@value": {
                "application/n-quads": "http://www.w3.org/ns/formats/N-Quads",
                "application/trig": "http://www.w3.org/ns/formats/TriG",
                "application/n-triples": "http://www.w3.org/ns/formats/N-Triples",
                "text/turtle": "http://www.w3.org/ns/formats/Turtle",
                "text/n3": "http://www.w3.org/ns/formats/N3"
              }
            }
          ],
          "comment": "A record of media types, with media type name as key, and its format IRI as value.",
          "unique": true,
          "required": true
        },
        {
          "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3_args_priorityScale",
          "range": "xsd:double",
          "comment": "A multiplier for media type priorities. This can be used for keeping the original media types in place, but scaling all of their scores with a certain value.",
          "unique": true
        },
        {
          "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3_args_name",
          "range": "xsd:string",
          "default": [
            {
              "@id": "rdf:subject"
            }
          ],
          "comment": "The name for this actor.",
          "unique": true,
          "required": true
        },
        {
          "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3_args_bus",
          "range": "cc:lib/Bus#Bus",
          "default": [
            {
              "@id": "cbrs:lib/ActorRdfSerialize#ActorRdfSerialize_default_bus",
              "@type": "cc:lib/Bus#Bus"
            }
          ],
          "comment": "The bus this actor subscribes to.",
          "unique": true,
          "required": true
        },
        {
          "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3_args_beforeActors",
          "range": "cc:lib/Actor#Actor",
          "comment": "Actor that must be registered in the bus before this actor."
        }
      ],
      "constructorArguments": [
        {
          "@id": "carsn:lib/ActorRdfSerializeN3#ActorRdfSerializeN3_args__constructorArgument",
          "fields": [
            {
