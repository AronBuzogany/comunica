        {
          "@id": "cr:lib/Runner#Runner_actors",
          "range": "cc:lib/Actor#Actor",
          "comment": "The list of all actors that are part of the comunica workflow.",
          "required": true
        },
        {
          "@id": "cr:lib/Runner#Runner_busInit",
          "range": "cc:lib/Bus#Bus",
          "default": {
            "@id": "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/bus-init/lib/ActorInit#ActorInit_default_bus"
          },
          "comment": "The 'init' event bus.",
          "unique": true,
          "required": true
        }
      ],
      "constructorArguments": [
        {
          "@id": "cr:lib/Runner#Runner_busInit"
        },
        {
          "@id": "cr:lib/Runner#Runner_actors"
        }
      ]
    }
  ]
