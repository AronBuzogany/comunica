  ],
  "@id": "urn:comunica:my",
  "@type": "Runner",
  "actors": [
    {
      "@id": "urn:comunica:sparqlinit",
      "@type": "ActorInitSparql",
      "mediatorOptimizeQueryOperation": {
        "@id": "#mediatorOptimizeQueryOperation",
        "@type": "MediatorCombinePipeline",
        "bus": { "@id": "ActorOptimizeQueryOperation:_default_bus" }
      },
      "mediatorQueryOperation": {
        "@id": "cais:config/sets/sparql-queryoperators.json#mediatorQueryOperation"
      },
      "mediatorSparqlParse": {
        "@id": "#mediatorSparqlParse",
        "@type": "MediatorRace",
        "bus": { "@id": "ActorSparqlParse:_default_bus" }
      },
      "mediatorSparqlSerialize": {
        "@id": "#mediatorSparqlSerialize",
        "@type": "MediatorRace",
        "bus": { "@id": "ActorSparqlSerialize:_default_bus" }
      },
      "mediatorSparqlSerializeMediaTypeCombiner": {
        "@type": "MediatorCombineUnion",
        "bus": { "@id": "ActorSparqlSerialize:_default_bus" },
        "field": "mediaTypes"
      },
      "mediatorSparqlSerializeMediaTypeFormatCombiner": {
        "@type": "MediatorCombineUnion",
        "bus": { "@id": "ActorSparqlSerialize:_default_bus" },
        "field": "mediaTypeFormats"
      },
      "mediatorContextPreprocess": {
        "@id": "#mediatorContextPreprocess",
        "@type": "MediatorCombinePipeline",
        "bus": { "@id": "ActorContextPreprocess:_default_bus" }
      },
      "mediatorHttpInvalidate": {
        "@id": "#mediatorHttpInvalidate",
        "@type": "MediatorAll",
        "bus": { "@id": "ActorHttpInvalidate:_default_bus" }
      }
    }
  ]
}
