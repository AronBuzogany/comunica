{
  "name": "@comunica/actor-init-sparql-rdfjs",
  "version": "1.22.3",
  "description": "A SPARQL query engine for querying over RDF/JS sources",
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/@comunica/actor-init-sparql-rdfjs/^1.0.0/config/": "config/"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/actor-init-sparql-rdfjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "sparql",
    "engine",
    "query",
    "update",
    "sparql engine",
    "sparql query",
    "sparql update",
    "sparql 1.1",
    "actor",
    "init",
    "rdfjs",
    "rdf/js"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "config",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "engine-default.js"
  ],
  "dependencies": {
    "@comunica/actor-init-sparql": "^1.22.3",
    "@comunica/runner-cli": "^1.22.0",
    "sparqlalgebrajs": "^4.0.0"
  },
  "scripts": {
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls",
    "prepare": "comunica-compile-config config/config-default.json > engine-default.js",
    "browser": "npm run prepare && \"../../node_modules/webpack/bin/webpack.js\" --config webpack.config.js --mode production",
    "integration": "rdf-test-suite-ldf spec/sparql-engine.js https://comunica.github.io/manifest-ldf-tests/sparql-rdfjs/rdfjs-manifest.ttl -d 200000 -c ../../.rdf-test-suite-ldf-cache/"
  },
  "browser": {
    "./lib/index.js": "./lib/index-browser.js"
  }
}
