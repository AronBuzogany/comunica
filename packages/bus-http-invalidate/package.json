{
  "name": "@comunica/bus-http-invalidate",
  "version": "1.22.0",
  "description": "A comunica bus for http-invalidate events.",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/bus-http-invalidate"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "bus",
    "http-invalidate"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "dependencies": {
    "@comunica/core": "^1.22.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "npm run build:ts && npm run build:components-bus",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "validate": "npm ls",
    "build:components-bus": "componentsjs-generator -s lib --typeScopedContexts -i ../../.componentsignore"
  }
}
