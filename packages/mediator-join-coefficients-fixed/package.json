{
  "name": "@comunica/mediator-join-coefficients-fixed",
  "version": "2.0.0",
  "description": "A join-coefficients-fixed mediator",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/mediator-join-coefficients-fixed"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "runner"
  ],
  "license": "MIT",
  "bugs": {
