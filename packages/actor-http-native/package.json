{
  "name": "@comunica/actor-http-native",
  "version": "2.0.0",
  "description": "A native http actor",
  "lsd:module": true,
  "main": "lib/index.js",
  "typings": "lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/comunica/comunica.git",
    "directory": "packages/actor-http-native"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "comunica",
    "actor",
    "http",
    "follow-redirects"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/comunica/comunica/issues"
  },
  "homepage": "https://comunica.dev/",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "peerDependencies": {
    "@comunica/bus-http": "^1.0.0",
    "@comunica/core": "^1.0.0"
  },
  "dependencies": {
    "@comunica/context-entries": "^2.0.0",
    "@types/parse-link-header": "^1.0.0",
    "cross-fetch": "^3.0.5",
    "follow-redirects": "^1.5.1",
    "parse-link-header": "^1.0.1"
  },
  "devDependencies": {
    "@comunica/bus-http": "^2.0.0",
    "@comunica/core": "^2.0.0",
    "@comunica/mediatortype-time": "^2.0.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "build": "npm run build:ts && npm run build:components",
    "build:ts": "node \"../../node_modules/typescript/bin/tsc\"",
    "build:components": "componentsjs-generator",
    "validate": "npm ls"
  },
  "browser": {
    "./lib/Requester.js": "./lib/Requester-browser.js"
  }
}
